{
    "global_attributes": {
        "purpose": "Hypoxia Monitoring",
        "description": "ASV hypoxia monitoring CTD data and QARTOD flags",
        "vehicle": "C-Worker 5",
        "source": "L3 Harris",
        "PI": "Steven Howden",
        "author": "Integral Consulting Inc",
        "time_zone": "PST",
        "convensions": "CF-1.6",
        "seasonal_params": {
            "spring": {
                "parameters": "./parameters/Sensor_Parameters.xlsx"
            },
            "summer": {
                "parameters": "./parameters/Sensor_Parameters.xlsx"
            },
            "fall": {
                "parameters": "./parameters/Sensor_Parameters.xlsx"
            },
            "winter": {
                "parameters": "./parameters/Sensor_Parameters.xlsx"
            }
        }
    },
    "standard_names": {
        "TEMPERATURE;C": "sea_water_temperature",
        "CONDUCTIVITY;MS/CM": "sea_water_electrical_conductivity",
        "PRESSURE;DBAR": "sea_water_pressure",
        "Calc. SALINITY; PSU": "sea_water_practical_salinity",
        "PH;PH": "sea_water_ph",
        "DISSOLVED OXYGEN;SAT%": "sea_water_dissolved_oxygen",
        "FLUOROMETER (C);UG/L": "sea_water_fluorescence",
        "TURBIDITY;FTU": "sea_water_turbidity",
        "ALTITUDE;M": "altitude",
        "Calc. DEPTH;M": "depth",
        "Date / Time": "time",
        "LATITUDE;DEG": "lat",
        "LONGITUDE;DEG": "lon"
    },
    "qc_tests": {
        "global": {
            "utils": {
                "check_timestamps": {
                    "max_time_interval": 3
                }
            },
            "qartod": {
                "location_test": {
                    "bbox": [
                        -98.000001,
                        18.000001,
                        -80.000001,
                        30.000001
                    ]
                }
            }
        },
        "parameters": {
            "sea_water_pressure": {
                "argo": {
                    "pressure_increasing_test": null
                },
                "axds": {
                    "valid_range_test": {
                        "valid_span": [3, 20]
                    }
                },
                "qartod": {
                    "gross_range_test": {
                        "suspect_span": [-2, 3],
                        "fail_span": [3, 20]
                    },
                    "flat_line_test": {
                        "tolerance": 0.001,
                        "suspect_threshold": 300,
                        "fail_threshold": 900
                    },
                    "rate_of_change_test": {
                        "threshold": 5
                    },
                    "spike_test": {
                        "suspect_threshold": 0.01,
                        "fail_threshold": 0.1,
                        "method": "average"
                    },
                    "attenuated_signal_test": {
                        "suspect_threshold": 5,
                        "fail_threshold": 3,
                        "test_period": 15,
                        "min_obs": null,
                        "min_period": null,
                        "check_type": "range"
                    },
                    "density_inversion_test": {
                        "suspect_threshold": 3,
                        "fail_threshold": 5
                    },
                    "climatology_test": {
                        "suspect_span": [-2, 3],
                        "fail_span": [3, 20]
                    }
                }
            },
            "sea_water_temperature": {
                "axds": {
                    "valid_range_test": {
                        "valid_span": [-5, 35]
                    }
                },
                "qartod": {
                    "gross_range_test": {
                        "suspect_span": [20, 35],
                        "fail_span": [-5, 35]
                    },
                    "flat_line_test": {
                        "tolerance": 0.01,
                        "suspect_threshold": 300,
                        "fail_threshold": 900
                    },
                    "rate_of_change_test": {
                        "threshold": 5
                    },
                    "spike_test": {
                        "suspect_threshold": 0.01,
                        "fail_threshold": 0.02,
                        "method": "average"
                    },
                    "attenuated_signal_test": {
                        "suspect_threshold": 5,
                        "fail_threshold": 3,
                        "test_period": 15,
                        "min_obs": null,
                        "min_period": null,
                        "check_type": "range"
                    },
                    "density_inversion_test": {
                        "suspect_threshold": 3,
                        "fail_threshold": 5
                    },
                    "climatology_test": {
                        "suspect_span": [-2, 3],
                        "fail_span": [3, 20]
                    }
                }
            }
        }
    }
}
