        },
        "PRESSURE;DBAR": {
            "standard_name": "sea_water_pressure",
            "long_name": "Pressure",
            "dtype": "float64",
            "original_units": "dbar",
            "units": "dbar"
        },
        "Calc. SALINITY; PSU": {
            "standard_name": "sea_water_practical_salinity",
            "long_name": "Practical salinity",
            "dtype": "float64",
            "original_units": "1e-3",
            "units" : "1e-3"
        },
        "PH;PH": {
            "standard_name": "sea_water_ph_reported_on_total_scale",
            "long_name": "pH",
            "dtype": "float64",
            "original_units": "1",
            "units": "1"
        },
