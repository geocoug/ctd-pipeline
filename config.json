            "standard_name": "time",
            "long_name": "Time",
            "dtype": "datetime64[s]",
            "units": "seconds since 1970-01-01 00:00:00",
            "calendar": "standard"
        },
        "LATITUDE;DEG": {
            "standard_name": "latitude",
            "long_naxszme": "Latitude degrees",
            "axis": "Y",
            "dtype": "float64",
            "units": "degrees_north"
        },
        "LONGITUDE;DEG": {
            "standard_name": "longitude",
            "long_name": "Longitude degrees",
            "axis": "X",
            "dtype": "float64",
            "units": "degrees_east"
        }
