    "ioos_qc": {
        "global": {
            "utils": {
                "check_timestamps": {
                    "max_time_interval": 3
                }
            },
            "qartod": {
                "location_test": {
                    "bbox": [
                        -98.000001,
                        18.000001,
                        -80.000001,
                        30.000001
                    ]
                }
            }
        },
        "variables": {
            "sea_water_pressure": {
                "argo": {
                    "pressure_increasing_test": null
                },
                "axds": {
                    "valid_range_test": {
                        "valid_span": [0, 30]
                    }
                },
                "qartod": {
                    "gross_range_test": {
                        "suspect_span": [0.01, 30],
                        "fail_span": [0.01, 30]
                    },
                    "flat_line_test": {
                        "tolerance": 0.001,
                        "suspect_threshold": 300,
                        "fail_threshold": 900
                    },
                    "rate_of_change_test": {
                        "threshold": 5
                    },
                    "spike_test": {
                        "suspect_threshold": 0.01,
                        "fail_threshold": 0.1,
                        "method": "average"
                    },
                    "attenuated_signal_test": {
                        "suspect_threshold": 5,
                        "fail_threshold": 3,
                        "test_period": 15,
                        "min_obs": null,
                        "min_period": null,
                        "check_type": "range"
                    },
                    "density_inversion_test": {
                        "suspect_threshold": 3,
                        "fail_threshold": 5
                    },
                    "climatology_test": {
                        "suspect_span": [-2, 3],
                        "fail_span": [3, 20],
