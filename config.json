    "ioos_qc": {
        "global": {
            "utils": {
                "check_timestamps": {
                    "max_time_interval": 3
                }
            },
            "qartod": {
                "location_test": {
                    "bbox": [
                        -98.000001,
                        18.000001,
                        -80.000001,
                        31.000001
                    ]
                }
            }
        },
        "variables": {
            "sea_water_pressure": {
                "argo": {
                    "pressure_increasing_test": {}
                },
                "axds": {
                    "valid_range_test": {
                        "valid_span": [0, 30]
                    }
                },
                "qartod": {
                    "gross_range_test": {
                        "suspect_span": [0.1, 25],
                        "fail_span": [0, 30]
                    },
                    "flat_line_test": {
                        "tolerance": 0.001,
                        "suspect_threshold": 300,
                        "fail_threshold": 900
                    },
                    "rate_of_change_test": {
                        "threshold": 1
                    },
                    "spike_test": {
                        "suspect_threshold": 0.01,
                        "fail_threshold": 0.1,
                        "method": "differential"
                    },
                    "attenuated_signal_test": {
                        "suspect_threshold": 0.01,
                        "fail_threshold": 0,
                        "test_period": 60,
                        "min_obs": null,
                        "min_period": null,
                        "check_type": "std"
                    },
                    "density_inversion_test": {
                        "suspect_threshold": 0.01,
                        "fail_threshold": 0
                    },
                    "climatology_test": {
                        "suspect_span": [0.1, 25],
                        "fail_span": [0, 30],
                        "zspan": [0, 100]
                    }
