        "TEMPERATURE;C": {
            "standard_name": "sea_water_temperature",
            "long_name": "Temperature",
            "dtype": "float64",
            "original_units": "deg_C",
            "units": "K"
        },
        "CONDUCTIVITY;MS/CM": {
            "standard_name": "sea_water_electrical_conductivity",
            "long_name": "Electrical Conductivity",
